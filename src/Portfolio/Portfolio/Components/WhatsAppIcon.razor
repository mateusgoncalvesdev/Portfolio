@inject IJSRuntime Js

<RadzenLink id="whatsappLink" Target="_blank">
    <iconify-icon icon="logos:whatsapp-icon" width="@Width" style="display:inline-block;"></iconify-icon>
</RadzenLink>

@code {
    [Parameter]
    public int Width { get; set; }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await Js.InvokeVoidAsync("injectWhatsappLink", "whatsappLink");
        }
    }
}
